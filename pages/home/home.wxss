@import "../tpls/banner/banner-tpl.wxss";
.page{
    width: 100%;
    height: 100vh;  
    position: relative;
}
.map{
   width:100%;
   height:100vh;
}
.search-box{
   position: fixed;
   top:109rpx;
   left:26rpx;
   z-index:999;
}
.search-box image{
    width:101rpx;
    height:101rpx;
}
.top-menu{
    position: fixed;
    top:109rpx;
    right:26rpx;
    z-index:999;
    display:flex;
    flex-direction: column;
}
.location-box{
    position: fixed;
    left:26rpx;
    bottom:192rpx;
    z-index:999;
}
.location-box image{
    width:101rpx;
    height:101rpx;
}
.top-menu image{
    width:101rpx;
    height:101rpx;
}
.top-menu image+image{
    margin-top:18rpx;
}

.customCallout {
    box-sizing: border-box;
    /* background-color: #fff; 
    border-radius: 30px;
    width: 150px;
    height: 40px;
    display: inline-flex;
    padding: 5px 20px;
    justify-content: center;
    align-items: center; */
}
.customCallout-image-wrapper{
   width:606rpx;
   height:286rpx; 
   background: #d5d3d3;
   border-radius:20rpx;
   padding:18rpx;
   position: relative;
} 
.customCallout-image-wrapper  .customCallout-image{
    width:100%;
    height:100%;
}
.customCallout-bottom-btns{
    display:flex;
    padding:0 27rpx;
    /* position: absolute; */
    bottom:-10px;
    left:7px;
}
.customCallout-bottom-btns .btn-wrapper{
    justify-content:center;
}
.customCallout-bottom-btns .btn{
    width:265rpx;
    height:60rpx;
    line-height:60rpx;
    text-align:center;
    background: #b8adad;
    border-radius:20rpx;
    font-size:30rpx;
    z-index:999;
}
.customCallout-bottom-btns .btn:first-child{
    margin-right:24rpx;
}
.marker-list-view{
    position: fixed;
    width:117rpx;
    height:440rpx;
    background-size:cover;
    box-sizing:border-box;
    z-index:999;
    right:36rpx;
    bottom:231rpx;
    padding:0 5rpx;
    display:flex;
    justify-content:center;
}
.marker-list-scroll{
    height:100%;
    background-image: url('data:image/png;base64,');
}
.pos-img{
    position: absolute;
    /* top:-13rpx; */
    width:117rpx;
    height:25rpx;
    z-index:9999;
}
.pos-img-top{
    top:-13rpx;
}
.pos-img-bottom{
    bottom:-13rpx;
}
.scroll-view-item{
    display:flex;
    justify-content:center;
    padding:50rpx 0rpx;
}
.scroll-view-item image{
    width:57rpx;
    height:57rpx;
}